# Neynar API Key (required for Farcaster API access)
NEYAR_API_KEY=your-neynar-api-key

# Cloudflare R2 Settings (required for image storage)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
CLOUDFLARE_R2_ACCESS_KEY=your-r2-access-key
CLOUDFLARE_R2_SECRET_KEY=your-r2-secret-key

# Cloudflare KV Settings (required for caching)
KV_BINDING=your-kv-namespace-id
CF_ACCOUNT_ID=your-cloudflare-account-id
CF_API_TOKEN=your-cloudflare-api-token

# Base URL (required for frame metadata)
NEXT_PUBLIC_BASE_URL=http://localhost:3000 